{
  "name": "Earth Backend Dev",
  "dockerComposeFile": [
    "../docker-compose.yaml",
    "docker-compose.dev.yml"
  ], 
  "service": "api", 
  "runServices": ["db", "api"], 
  "workspaceFolder": "/app", 
  "remoteUser": "vscode",
  "overrideCommand": true,
  "forwardPorts": [8000, 5432],

  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-azuretools.vscode-docker"
      ]
    }
  }
}