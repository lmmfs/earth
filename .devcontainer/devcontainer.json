{
  "name": "Earthquake Backend Dev",
  "dockerFile": "Dockerfile",
  "context": "..",
  "workspaceFolder": "/workspace",
  "mounts": [
    "source=${localWorkspaceFolder},target=/workspace,type=bind"
  ],
  "service": "dev",
  "runServices": ["db"],
  "dockerComposeFile": "../docker-compose.yml",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "forwardPorts": [8000],
  "postCreateCommand": "pip install -r /workspace/api/requirements.txt",
  "remoteUser": "vscode"
}